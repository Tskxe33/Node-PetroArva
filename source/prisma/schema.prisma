datasource db {
  provider = "sqlite"
  url      = "file:./app.db"
}

generator client {
  provider = "prisma-client-js"
}

model Booking {
  id             Int      @id @default(autoincrement())
  guestName      String
  unitID         String
  checkInDate    DateTime
  checkOutDate   DateTime
  numberOfNights Int
}
